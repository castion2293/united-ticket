{
  "name": "super-platform/united-ticket",
  "description": "United Ticket",
  "type": "library",
  "authors": [
    {
      "name": "Liao San Kai",
      "email": "liaosankai@js-tech.tw"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@git.sp168.cc:super-platform/laravel-package/api-caller.git"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/database": "~5.3",
    "illuminate/support": "~5.3",
    "gazugafan/laravel-temporal": "^1.1",
    "guzzlehttp/guzzle": "^6.3",
    "super-platform/api-caller": "^1.10",
    "ext-json": "*",
    "symfony/dom-crawler": "^4.3"
  },
  "require-dev": {
    "illuminate/http": "~5.3",
    "illuminate/routing": "~5.3",
    "phpunit/phpunit": "^7",
    "vlucas/phpdotenv": "~2.0",
    "orchestra/testbench": "3.6.*",
    "orchestra/database": "3.6.*",
    "mockery/mockery": "^1"
  },
  "autoload": {
    "psr-4": {
      "SuperPlatform\\UnitedTicket\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SuperPlatform\\UnitedTicket\\": "tests/"
    },
    "classmap": [
      "tests/",
      "database/migrations/",
      "database/factories/"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "SuperPlatform\\UnitedTicket\\UnitedTicketServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
